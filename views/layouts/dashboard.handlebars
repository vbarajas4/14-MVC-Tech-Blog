 <title>The Tech Blog</title>
  <link rel="stylesheet" href="/css/style.css">
</head>

<body>
  <div class="wrapper">
    <header>
      <div>
        <nav>
          <a href="/">Homepage</a>
          <a href="/dashboard">Dashboard</a> 
          {{#if logged_in}}
          <a href="/" id="logout-link">Logout</a>
          {{else}}
          <a href="/login">Login</a>
          {{/if}}
        </nav>
      </div>

      <div>
        <h1 class="app-title"><a href="/dashboard">Your Dashboard</a></h1>
      </div>

    </header>

    <main>
      {{{ body }}}
    </main>

    <footer class="footer flex-row justify-center">
    <a href="/dashboard/new">➕ New Post</a>
  </footer>
  </div>

  <!-- This will render script for logged in users -->
  {{#if logged_in}}
  <script src="/js/logout.js"></script>
  {{/if}}
</body>

</html>











